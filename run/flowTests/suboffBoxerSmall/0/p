FoamFile
{
    version         2.0;
    format          ascii;
    class           volScalarField;
    object          p;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions [0 2 -2 0 0 0 0];

internalField uniform 0;

boundaryField
{
    oversetFaces
    {
        type overset;
        value uniform 0;
        coupledFringe yes;

        setHoleCellValue  yes;
        holeCellValue     0;
    }
    x_min
    {
        type zeroGradient;
    }
    x_max
    {
        type zeroGradient;
    }
    y_min
    {
        type zeroGradient;
    }
    y_max
    {
        type zeroGradient;
    }
    z_min
    {
        type zeroGradient;
    }
    z_max
    {
        type zeroGradient;
    }
    hull
    {
        type zeroGradient;
    }
    sailAll
    {
        type zeroGradient;
    }
    sternFins
    {
        type zeroGradient;
    }
    inlet
    {
        type zeroGradient;
    }
    outlet
    {
//         type zeroGradient;

        type fixedValue;
        value uniform 0;
    }
    sides
    {
        type zeroGradient;
    }
}


