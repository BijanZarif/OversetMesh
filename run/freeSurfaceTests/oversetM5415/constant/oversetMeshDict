FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          oversetMeshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolation
{
    type injection;
}

// Names or patches that determine hole cells
holePatches ();

regions
(
    background
    {
        // Donor regions
        donorRegions ( hull );

        fringe
        {
            type manual;
            holes "backHole";
            acceptors "backFringe";
        }
//         fringe
//         {
//             type overlap;
//         }
    }
    hull
    {
        donorRegions ( background );

        holePatches ( bilgeKeel deck hull);

        fringe
        {
            type faceCells;
            patches ( x_min x_max y_min z_min z_max);
        }
    }
);


