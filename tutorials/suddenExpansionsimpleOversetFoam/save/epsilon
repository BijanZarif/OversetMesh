FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.0002;

boundaryField
{
    oversetFaces
    {
        type            overset;
        value           uniform 0.0002;
        coupledFringe yes;

        setHoleCellValue  yes;
        holeCellValue     1e-10;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 0.0002;
    }
    leftOut
    {
        type            zeroGradient;
    }
    inletWalls
    {
        type            epsilonWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0.0002;
    }
    outletWalls
    {
        type            epsilonWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0.0002;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.0002;
        value           uniform 0.0002;
    }
    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
